{
  "name": "drnixx/yii2-kendo-widgets",
  "version": "1.0.1",
  "description": "Yii2 widgets for KendoUI components",
  "type": "library",
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/DrNixx/yii2-kendo"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "drnixx/yii2-kendo": "dev-master",
    "yiisoft/yii2": "~2.0.6",
    "bower-asset/bootstrap": "~4.0.0@beta",
    "bower-asset/popper.js": "~1.12.0",
    "yiisoft/yii2-bootstrap": "2.1.x-dev",
    "fxp/composer-asset-plugin": "v1.4.1"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Nick Panteleeff",
      "email": "drnix@chess-online.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "yii2\\kendo\\": "src/"
    }
  },
  "config": {
    "platform": {"php": "5.6"},
    "fxp-asset":{
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  }
}
